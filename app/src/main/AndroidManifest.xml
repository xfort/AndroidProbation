<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.xiushuang.xprobation"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name=".AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".sample.VideoJSActivity">
        </activity>
        <activity android:name=".sample.X5WebViewSDK">
        </activity>
        <activity android:name=".sample.XCrossWalkActivity">
        </activity>
        <activity android:name=".sample.CordovaActivity">
        </activity>
    </application>

</manifest>